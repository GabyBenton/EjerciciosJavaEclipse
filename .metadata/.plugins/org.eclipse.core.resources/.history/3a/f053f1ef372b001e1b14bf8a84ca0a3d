
public class WrapperTest {
	 public static void main(String[] args) {
		System.out.println("Datos bytes");
		System.out.println(Byte.BYTES);
		System.out.println(Byte.SIZE);
		System.out.println(Byte.MIN_VALUE);
		System.out.println(Byte.MAX_VALUE);
		
		System.out.println("Datos short");
		System.out.println(Short.BYTES);
		System.out.println(Short.SIZE);
		System.out.println();
		System.out.println();
		
		
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
	}
}
