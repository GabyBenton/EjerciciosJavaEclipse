import java.util.*;

public class EjerciciosPrueba {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("-----------Ejercicio 1------------");
		for (int i = 1; i <=3; i++) {
			for (int y = 1; y <=3; y++) {
				System.out.println(i+""+y);
			}
		}
		
		
		System.out.println("-----------Ejercicio 1------------");
		Scanner entradita = new Scanner(System.in);
		System.out.println("Ingresa el número de filas: ");
		int filas = entradita.nextInt();
		System.out.println("Ingresa el número de columnas: ");
		int columnas = entradita.nextInt();
		String piramide = "";
		for (int i = 0; i < filas; i++) {
			for (int asteriscos = 0; asteriscos < filas; asteriscos++) {
				System.out.println(piramide +" *");
			}
		}
		
		for (int i = 0; i < columnas; i++) {
			for (int asteriscos = 0; asteriscos < columnas; asteriscos++) {
				System.out.println(piramide +" *");
			}
		}
	}

}
