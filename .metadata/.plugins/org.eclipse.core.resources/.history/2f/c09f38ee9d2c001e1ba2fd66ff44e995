/**
 * 
 */
public class For {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for (int cont = 0; cont < 10; cont++) {
			System.out.println(cont);
		}//for cont
		
		//Ciclo para mostrar pares entre 0-100
		for (int num = 0; num <= 100; num++) {
			
		}
	}

}
