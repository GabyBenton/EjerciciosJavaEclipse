	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for (int i = 1; i <=3; i++) {
			System.out.println(i);
		}
		
		for (int y = 1; y <=3; y++) {
			System.out.println();
		}
		
		
		for(let num1=1;num1<=3;num1++){
		    for(let num=1;num<=3;num++){
		        console.log(num1+" "+num);
		    }//for renglon
		}
	}

}
